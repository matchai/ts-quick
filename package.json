{
  "name": "ts-quick",
  "version": "0.0.0-semantic-release",
  "description": "⚡️ Zero configuration static analysis using TypeScript",
  "license": "ISC",
  "repository": "matchai/ts-quick",
  "author": "Matan Kushner <hello@matchai.me>",
  "bin": "cli.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "./cli.js && jest"
  },
  "files": [
    "lib",
    "*.js"
  ],
  "keywords": [
    "typescript",
    "static",
    "analysis",
    "javascript",
    "precommit",
    "cli",
    "check",
    "checker",
    "code",
    "quality",
    "verify"
  ],
  "dependencies": {
    "arrify": "1.0.1",
    "debug": "4.1.1",
    "execa": "1.0.0",
    "globby": "8.0.1",
    "has-yarn": "1.0.0",
    "meow": "5.0.0",
    "pkg-conf": "2.1.0",
    "read-pkg-up": "4.0.0",
    "resolve-cwd": "2.0.0",
    "typescript": "3.2.2",
    "write-pkg": "3.2.0"
  },
  "devDependencies": {
    "@types/arrify": "1.0.4",
    "@types/debug": "0.0.31",
    "@types/execa": "0.9.0",
    "@types/globby": "8.0.0",
    "@types/meow": "5.0.0",
    "@types/node": "10.12.18",
    "@types/pkg-conf": "2.1.0",
    "@types/read-pkg-up": "3.0.1",
    "@types/write-pkg": "3.1.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0"
  },
  "ts-quick": {
    "ignore": "./test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "renovate": {
    "extends": [
      "config:base",
      ":rebaseStalePrs",
      ":automergeMinor"
    ]
  }
}
